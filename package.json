{
  "name": "rest-express",
  "homepage": "https://dpandey1105.github.io/Community-Connect",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "npx concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "set NODE_ENV=development&& set PORT=5000&& node backend/index.js",
    "dev:client": "vite",
    "build": "cross-env NODE_ENV=production&& vite build && esbuild backend/index.js --platform=node --packages=external --bundle --format=esm --outdir=dist babel src -d dist",
    "postbuild": "cp dist/index.html dist/404.html",
    "build:frontend": "vite build",
    "build:backend": "npx esbuild backend/index.js --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "set PORT=5000&& cross-env NODE_ENV=production node dist/index.js",
    "deploy:frontend": "npm run build:frontend && npm run postbuild && gh-pages -d dist",
    "deploy:backend": "npm run build:backend",
    "test": "jest",
    "start:prod": "node server.js"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ]
  },
